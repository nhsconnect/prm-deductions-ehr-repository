{
  "name": "prm-deductions-ehr-repository",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "lint": "eslint src/ test/ scripts/",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint:fix && npm run test:unit",
    "test:unit": "jest \"src/(?!(.*\\integration.test\\.js))\" --setupFiles dotenv/config",
    "test:integration": "jest \"src/.*\\integration.test\\.js\" --detectOpenHandles --forceExit --setupFiles dotenv/config",
    "test:integration:local": "npm run db:setup && npm run test:integration ; npm run db:teardown",
    "test:functional": "jest test/functional/*",
    "test:docker": "jest test/docker/*",
    "test:watch": "jest \"src/(?!(.*\\integration.test\\.js))\" --setupFiles dotenv/config --watch",
    "test:coverage": "jest src/ --maxWorkers=1 --coverage --detectOpenHandles --forceExit --setupFiles dotenv/config",
    "test:coverage:local": "npm run db:setup && npm run test:coverage ; npm run db:teardown",
    "build": "babel src -d build --ignore '**/*.test.js' --ignore '**/__mocks__/*' && cp src/*.json build",
    "start": "node build/server.js",
    "start:local": "npm run db:migrate ; babel-node -r dotenv/config src/server.js",
    "start:nodemon": "nodemon --exec npm run start:local",
    "db:wait": "babel-node scripts/wait-for-postgres.js",
    "db:migrate": "npm run db:wait && sequelize-cli db:migrate",
    "db:teardown": "sequelize-cli db:migrate:undo:all",
    "db:setup": "sequelize-cli db:migrate && npm run db:data:seed",
    "db:data:seed": "sequelize-cli db:seed:all",
    "db:data:remove": "sequelize-cli db:seed:undo:all"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "acorn": "^7.1.1",
    "axios": "^0.19.2",
    "dateformat": "^3.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.6",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.3",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "async-local-storage": "^2.3.1",
    "aws-sdk": "^2.639.0",
    "dayjs": "^1.8.23",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "express-winston": "^4.0.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "traverse": "^0.6.6",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "jest": {
    "resetModules": true,
    "clearMocks": true,
    "automock": false
  }
}
